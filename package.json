{
  "name": "api-ts-scaffold",
  "version": "0.1.0",
  "description": "Scaffold project for NS",
  "scripts": {
    "app:build-ts": "tsc",
    "app:start": "node dist/src/index.js",
    "app:build": "nodemon --exec ./node_modules/.bin/ts-node -- ./src/index.ts",
    "docker:start": "docker-compose up",
    "docker:stop": "docker-compose -f docker-compose.yml down -v --remove-orphans",
    "dev:start": "npm run app:build"
  },
  "keywords": [
    "api"
  ],
  "dependencies": {
    "@hapi/hapi": "^18.3.1",
    "@hapi/inert": "^5.2.1",
    "@hapi/joi": "^15.1.0",
    "@hapi/vision": "^5.5.2",
    "@sentry/node": "^5.6.2",
    "@types/code": "^4.0.5",
    "@types/dotenv": "^6.1.1",
    "@types/hapi": "^18.0.2",
    "@types/hapi__hapi": "^18.2.5",
    "@types/hapi__joi": "^15.0.3",
    "@types/joi": "^14.3.3",
    "@types/node": "^12.6.8",
    "dotenv": "^8.0.0",
    "hapi-swagger": "^10.0.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "nodemon": "^1.19.0",
    "prettier": "^1.18.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "author": "Gabriel C <shellcooking@gmail.com>"
}
